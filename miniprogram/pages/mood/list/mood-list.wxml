<wux-cell-group class="">
  <wux-swipe-action-group>
    <wux-cell-group
      wx:for="{{ moodList }}"
      wx:key="index"
      data-level="{{ item.level }}"
      title="{{ item.label }}">
      <wux-swipe-action 
        wx:for="{{ item.list }}"
        autoClose 
        right="{{ right }}"
        bind:click="handlerDeleteActive"
        data-index="{{ index }}">
        <wux-cell 
          title="{{ item.title }}" 
          label="{{ item.remark || '' }}"
          isLink="true"
          url="/pages/active/edit/edit-active?id={{ item.id }}&remark={{ item.remark }}&iconType={{ item.iconType }}&title={{ item.title }}">
          <active-icon groupType="active-business-icon" class="active-title-icon" size="16" slot="header" type="{{ item.iconType }}"></active-icon>
        </wux-cell>
      </wux-swipe-action>
    </wux-cell-group>
  </wux-swipe-action-group>
</wux-cell-group>
 
 <!-- 浮动按钮 -->
<wux-fab-button actionRotate="false" controlled="true" bind:actionClick="handlerAddActive" /> 
<wux-toptips id="wux-toptips" />